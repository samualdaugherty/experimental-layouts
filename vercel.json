{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/stoic-drama/(.*)",
      "dest": "/stoic-drama/$1"
    },
    {
      "src": "/algorithm/(.*)",
      "dest": "/algorithm/$1"
    },
    {
      "src": "/how-we-did-it/(.*)",
      "dest": "/how-we-did-it/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
